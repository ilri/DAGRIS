<VirtualHost *:80>
        ServerName dagris.local
        ServerAdmin me@example.com

        DocumentRoot "/var/www/html/dagris"

        <Directory "/var/www/html/dagris/">
                Options All
                AllowOverride All
                Order allow,deny
                Allow from all
        </Directory>

        ErrorLog "/var/log/apache2/error_log"
        TransferLog "/var/log/apache2/access_log"
</VirtualHost>
